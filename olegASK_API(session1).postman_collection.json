		{
			"name": "POST/sign-in (Successful student sign-in)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Successful POST request\", function () {",
							"    pm.expect(pm.response.code).to.be.oneOf([200]);",
							"});",
							"pm.test(\"User role validation\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.user.role).to.eql(\"STUDENT\");",
							"});",
							"",
							"pm.environment.set(\"studentToken\", pm.response.json().token);",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"email\": \"atwetdott@mailnesia.com\",\n  \"password\": \"123abc\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{baseUrl}}/sign-in",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"sign-in"
					]
				}
			},
			"response": []
		},
